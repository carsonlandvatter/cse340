<h1><%- title %></h1>
<%- messages() %>
<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>
  <p>You're logged in.</p>

  <a href="/account/update/<%= locals.accountData.account_id %>">Edit Account Information</a>
  <% if (locals.accountData.account_type === "Employee" || locals.accountData.accounttype === "Admin") { %>
    <h3>Inventory Management</h3>
    <p><a href="/inv/">Go to Inventory Management</a></p>
    <% } %>